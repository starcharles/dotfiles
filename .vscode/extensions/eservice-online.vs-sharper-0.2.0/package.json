{
	"name": "vs-sharper",
	"displayName": "VS Sharper for C#",
	"description": "C# IDE, Code Generation and Refactoring for VS Code",
	"icon": "vssharper-logo.png",
	"publisher": "eservice-online",
	"author": "eService Online",
	"version": "0.2.0",
	"preview": false,
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode development --env.development",
		"watch": "webpack --watch --mode development --env.development --info-verbosity verbose",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test",
		"package": "vsce package --yarn --out ./bin/vs-sharper-0-2-0.vsix --baseContentUrl https://raw.githubusercontent.com/sharperdev/vs-sharper/master/ --baseImagesUrl https://raw.githubusercontent.com/sharperdev/vs-sharper/master/",
		"install-extension": "code --install-extension ./bin/vs-sharper-0-2-0.vsix",
		"uninstall-extension": "code --uninstall-extension eservice-online.vs-sharper"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"homepage": "https://github.com/sharperdev/vs-sharper",
	"repository": {
		"type": "git",
		"url": "https://github.com/sharperdev/vs-sharper"
	},
	"bugs": {
		"url": "https://github.com/sharperdev/vs-sharper/issues"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Programming Languages",
		"Linters",
		"Snippets",
		"Formatters"
	],
	"keywords": [
		"c#,ide,code assistance,refactoring"
	],
	"main": "./dist/extension.js",
	"activationEvents": [
		"onLanguage:csharp",
		"workspaceContains:**/*.csproj",
		"workspaceContains:**/*.cs"
	],
	"contributes": {
		"languages": [
			{
				"id": "csharp",
				"aliases": [
					"C#"
				],
				"extensions": [
					".cs"
				]
			}
		],
		"commands": [
			{
				"command": "vsSharper.addCsharpClassFile",
				"title": "Add a New C# Class File",
				"category": "VS Sharper"
			},
			{
				"command": "vsSharper.addCsharpInterfaceFile",
				"title": "Add a New C# Interface File",
				"category": "VS Sharper"
			},
			{
				"command": "vsSharper.embedSelection",
				"title": "Embed Current Line or Selection",
				"category": "VS Sharper"
			},
			{
				"command": "vsSharper.embedSelection.block",
				"title": "Embed Current Line or Selection into Code Block",
				"category": "VS Sharper"
			},
			{
				"command": "vsSharper.sortUsings",
				"title": "Sort and group using statements",
				"category": "VS Sharper"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "vsSharper.addCsharpClassFile",
					"group": "vsSharperCsharp",
					"when": "vsSharperCsharp"
				},
				{
					"command": "vsSharper.addCsharpInterfaceFile",
					"group": "vsSharperCsharp",
					"when": "vsSharperCsharp"
				}
			],
			"editor/context": [
				{
					"command": "vsSharper.embedSelection",
					"group": "1_modification@-1",
					"when": "vsSharperCsharp"
				}
			]
		},
		"keybindings": [
			{
				"command": "vsSharper.embedSelection",
				"key": "ctrl+i e",
				"when": "vsSharperCsharp && editorFocus && !editorReadonly"
			},
			{
				"command": "vsSharper.embedSelection.block",
				"key": "ctrl+i b",
				"when": "vsSharperCsharp && editorFocus && !editorReadonly"
			}
		]
	},
	"dependencies": {
		"vscode-extension-telemetry": "^0.1.1",
		"vscode-jsonrpc": "^4.0.0"
	},
	"devDependencies": {
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42",
		"ts-loader": "^5.3.3",
		"tslint": "^5.14.0",
		"tslint-loader": "^3.5.4",
		"typescript": "^3.3.1",
		"vscode": "^1.1.30",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.3.0"
	},
	"__metadata": {
		"id": "842fbbac-70cb-42d6-a2e3-a9af9409fc25",
		"publisherId": "e798c4f9-90c9-4ebb-997e-323d9344c07c",
		"publisherDisplayName": "eService Online",
		"targetPlatform": "undefined",
		"updated": false,
		"isPreReleaseVersion": false,
		"installedTimestamp": 1689341048396,
		"preRelease": false
	}
}