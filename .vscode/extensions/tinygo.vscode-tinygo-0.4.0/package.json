{
	"name": "vscode-tinygo",
	"displayName": "TinyGo",
	"version": "0.4.0",
	"publisher": "tinygo",
	"description": "TinyGo support for Visual Studio Code",
	"license": "BSD-3-Clause",
	"icon": "images/tinygo-logo.png",
	"categories": [
		"Programming Languages"
	],
	"extensionDependencies": [
		"golang.go"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tinygo-org/vscode-tinygo"
	},
	"engines": {
		"vscode": "^1.48.0"
	},
	"activationEvents": [
		"onLanguage:go",
		"onCommand:vscode-tinygo.selectTarget",
		"onCommand:vscode-tinygo.showPreviewToSide",
		"onWebviewPanel:vscode-tinygo.preview"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-tinygo.selectTarget",
				"title": "TinyGo target"
			},
			{
				"command": "vscode-tinygo.showPreviewToSide",
				"title": "TinyGo Preview",
				"icon": "$(open-preview)"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceLangId == go && tinygoHasPreview",
					"command": "vscode-tinygo.showPreviewToSide",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-loader": "^8.0.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"__metadata": {
		"id": "3b00f2cc-1d75-4dd2-8c40-d02fc601b6fa",
		"publisherId": "417cc8d9-b76d-43b7-ad3e-e40ec1450eb8",
		"publisherDisplayName": "TinyGo",
		"targetPlatform": "undefined",
		"updated": false,
		"isPreReleaseVersion": false,
		"installedTimestamp": 1690335004224,
		"preRelease": false
	}
}